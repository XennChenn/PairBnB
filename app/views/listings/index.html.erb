<%= link_to 'Create Listing', new_listing_path %>

<table>
	<tr>
		<th>Title</th>
		<th>Bedroom</th>
		<th>Bathroom</th>
		<th>Checkin</th>
		<th>Checkout</th>
		<th>Number of Guest</th>
		<th>Description</th>
		<th>Rule</th>
		<th>Price</th>
	</tr>
	
	<% @listing.each do |x| %>
		<tr>
			<td><a href="/listings/<%= x.id %>"><%= x.title %></a></td>
			<td><%= x.bedroom %></td>
			<td><%= x.bathroom %></td>
			<td><%= x.checkin %></td>
			<td><%= x.checkout %></td>
			<td><%= x.number_of_guest %></td>
			<td><%= x.description %></td>
			<td><%= x.rule %></td>
			<td><%= x.price %></td>
		</tr>
	<% end %>
</table>